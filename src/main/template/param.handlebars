<td class='code'>{{name}}</td>
<td>

	{{#if isBody}}
		{{#if isFile}}
			<input type="file" name='{{name}}'/>
			<div class="parameter-content-type" />
		{{else}}
			{{#if defaultValue}}
				<textarea class='body-textarea' name='{{name}}'>{{defaultValue}}</textarea>
			{{else}}
				<textarea class='body-textarea' name='{{name}}'></textarea>
				<br />
				<div class="parameter-content-type" />
			{{/if}}
		{{/if}}
	{{else}}
		{{#if isQuery }}
			<input class='parameter' minlength='0' name='{{name}}' placeholder='' type='text' value='QUERY TYPE VALUE' disabled/>
		{{else}}
			{{#if defaultValue}}
				<input class='parameter' minlength='0' name='{{name}}' placeholder='' type='text' value='{{defaultValue}}'/>
			{{else}}
				<input class='parameter' minlength='0' name='{{name}}' placeholder='' type='text' value=''/>
			{{/if}}
		{{/if}}
	{{/if}}

</td>
<td>
	{{#if isQuery}}
		{{#each choices}}
			<input type="checkbox" name="queryparamchoice" value="{{this}}">
			<label for="{{this}}">{{this}}</label>
		{{/each}}
	{{else}}
		{{{description}}}
	{{/if}}
</td>
<td>{{{paramType}}}</td>
<td>
	<span class="model-signature"></span>
</td>
