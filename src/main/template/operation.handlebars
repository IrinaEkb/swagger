<div class="heading row">
  <div class="col-sm-12">
    <h4 class="human-readable-name">
      <a href='#!/{{parentId}}/{{nickname}}' class="toggleOperation">{{{summary}}}</a>
    </h4>
  </div>
</div>
<div class='content row' id='{{parentId}}_{{nickname}}_content' style='display:none'>
  {{#if notes}}
    <div class="col-sm-6">
      <p>{{{notes}}}</p>
    </div>
  {{/if}}
</div>
<div class="row">
  <div class="col-sm-12 options">
    <span class='http_method'>
    <a href='#!/{{parentId}}/{{nickname}}' class="toggleOperation">{{method}}</a>
    </span>
    <span class='path'>
    <a href='#!/{{parentId}}/{{nickname}}' class="toggleOperation">{{path}}</a>
    </span>
  </div>
</div>
<div class="content">
  {{#oauth}}
  <div class="auth">
  <span class="api-ic ic-error"></span>{{/oauth}}
  {{#each oauth}}
    <div id="api_information_panel">
    {{#each this}}
      <div title='{{{this.description}}}'>{{this.scope}}</div>
    {{/each}}
    </div>
  {{/each}}
  {{#oauth}}</div>{{/oauth}}
  {{#oauth}}
  <div class='access'>
    <span class="api-ic ic-off" title="click to authenticate"></span>
  </div>
  {{/oauth}}
  <form accept-charset='UTF-8' class='sandbox'>
    <div style='margin:0;padding:0;display:inline'></div>
    {{#if parameters}}
    <div class="row">
      <div class="col-sm-12">
        <h5>Parameters</h5>
      </div>
    </div>
    <div class="operation-params">

    </div>
    {{/if}}
    <div class="row">
      <div class="col-sm-6">
        <button class='c-button-secodary submit {{method}}' name='commit' type='button submit'>Submit {{method}}</button>
      </div>
    </div>
    {{#if responseMessages}}
    <div style='margin:0;padding:0;display:inline'></div>
    <div class="row">
      <div class="col-sm-12">
        <h5>Response Messages</h5>
      </div>
    </div>
    <table class='fullwidth'>
      <thead>
      <tr>
        <th>HTTP Status Code</th>
        <th>Reason</th>
        <th>Response Model</th>
      </tr>
      </thead>
      <div class="col-sm-6 operation-status">
      
      </div>
    </table>
    {{/if}}
    {{#if type}}
    <div class="row">
      <div class="col-sm-12">
        <h5>Response Class</h5>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <p><span class="model-signature" /></p>
      </div>
    </div>
    {{/if}}
    <div class='sandbox_header'>
      <input class='response_hider' type='button' style='display:none' value='Hide Response' href='#'/>
    </div>
  </form>
  <div class="row">
    <div class='response col-sm-12' style='display:none'>
      <h6>Request URL</h6>
      <div class='block request_url'></div>
      <h6>Response Body</h6>
      <div class='block response_body'></div>
      <h6>Response Code</h6>
      <div class='block response_code'></div>
      <h6>Response Headers</h6>
      <div class='block response_headers'></div>
    </div>
  </div>
</div>
