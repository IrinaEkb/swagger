<div class="row param-complex-query">
  <div class="col-sm-6">
    <div class="row">
      <div class="col-sm-12 param-code">
        <label class="code required c-input-label" for="textInput">{{name}}</label>
        <div class="data-type">
        {{#if isBody}}
          <span class='model-signature'></span>
        {{else}}
          <span class='data-type'></span>
        {{/if}}
        </div>
        <label class="c-input-label">{{{paramType}}}</label>
        <form class='query-choices'></form>
      </div>
    </div>
  </div>
</div>
